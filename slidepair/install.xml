<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/moodlecst/db" VERSION="20150108" COMMENT="XMLDB file for Moodle mod/moodlecst"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="moodlecst_items" COMMENT="moodlecst_items table retrofitted from MySQL">
      <FIELDS>
